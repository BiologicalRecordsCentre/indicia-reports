    SELECT
      cof_filtered_cof.id,
      cof_filtered_cof.taxa_taxon_list_id
    FROM cache_occurrences_functional cof_filtered_cof
    JOIN cache_taxa_taxon_lists cof_filtered_cttl on cof_filtered_cttl.id = cof_filtered_cof.taxa_taxon_list_id
    WHERE cof_filtered_cof.website_id IN (#count_website_list#) AND
    (
      (
        lower('#species_list#') = 'wildflower'
        AND cof_filtered_cttl.organism_key IN (SELECT organism_key FROM wildflower_organism_keys)
      )
      OR
      (
        lower('#species_list#') = 'indicator'
        AND cof_filtered_cttl.organism_key IN (SELECT organism_key FROM indicator_organism_keys)
      )
      OR
      (
        (lower('#species_list#') = 'inventory' OR lower('#species_list#') = 'additional')
        AND cof_filtered_cttl.taxon_group_id IN (78, 81, 87, 99, 89, 94, 137, 148)
      )
      OR
      (
        (lower('#species_list#') = 'standard')
        AND cof_filtered_cttl.taxon_group_id IN (78,81,87,99,89,94,137,148,155,133,129,97,227,128)
      )
    )
    LIMIT #occ_row_limit#
    )
    SELECT #columns#
    FROM cttl_preferred
    LEFT JOIN cttl_same_meaning on cttl_same_meaning.taxon_meaning_id = cttl_preferred.taxon_meaning_id
    JOIN cache_taxon_searchterms cts on cts.taxa_taxon_list_id = cttl_preferred.id and cts.simplified=false
    LEFT JOIN cof_filtered cof ON cof.taxa_taxon_list_id = cttl_same_meaning.id
    WHERE
    (
      lower('#species_list#') = 'wildflower'
      AND cttl_preferred.organism_key IN (SELECT organism_key FROM wildflower_organism_keys)
    )
    OR
    (
      lower('#species_list#') = 'indicator'
      AND cttl_preferred.organism_key IN (SELECT organism_key FROM indicator_organism_keys)
    )
    OR
    (
      (lower('#species_list#') = 'inventory' OR lower('#species_list#') = 'additional')
      AND cttl_preferred.taxon_group_id IN (78, 81, 87, 99, 89, 94, 137, 148)
    )
    OR
    (

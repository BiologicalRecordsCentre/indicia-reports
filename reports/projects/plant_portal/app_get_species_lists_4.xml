  </order_bys>
  <params>
    <param name="species_list" display="species_list" datatype="text" />
    <param name="count_website_list" display="Count Website List" description="Website IDs to include in count" datatype="text" default='32' emptyvalue='32'/>
    <param name="occ_row_limit" display="Occurrence Row Limiter" description="Limit the number of occurrence rows to count" datatype="text" default='50000' emptyvalue='50000'/>
  </params>
  <columns>
    <column name='id' display='id' sql="cttl_preferred.id" datatype='integer' in_count="true" />
    <column name='taxon_group' display='taxon_group' sql="cttl_preferred.taxon_group_id" datatype='integer'  />
    <column name='taxon' display='taxon' sql="cttl_preferred.taxon" datatype='text' />
    <column name='common_name' display='common_name' sql="cttl_preferred.default_common_name" datatype='text' />
    <column name='synonym' display='synonym' sql="
      jsonb_object_agg(distinct cttl_same_meaning.id, cttl_same_meaning.taxon
      ORDER BY cttl_same_meaning.taxon ASC)
      filter (where cttl_same_meaning.id is not null
        AND cttl_same_meaning.language_iso = 'lat'
        AND cttl_same_meaning.taxon != cttl_preferred.taxon)" datatype='text' aggregate="true" />
    <column name='organism_key' display='organism_key' sql="cttl_preferred.organism_key" datatype='text' />
    <column name='difficulty' display='difficulty' sql="cts.identification_difficulty" datatype='integer' />
    <column name='frequency' display='frequency' sql="count(distinct cof.id)" datatype='integer' aggregate="true" />
  </columns>
</report>
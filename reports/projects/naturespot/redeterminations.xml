<?xml version="1.0" encoding="UTF-8"?>
<report title="Redeterminations"
    description="Lists redeterminations of NS records.">
  <query website_filter_field="">
    select #columns#
    from cache_occurrences_functional o
    join determinations d on d.occurrence_id=o.id and d.deleted=false
    join cache_taxa_taxon_lists cttl on cttl.id=d.taxa_taxon_list_id
    where o.website_id=8
  </query>
  <order_bys>
    <order_by>o.id ASC</order_by>
  </order_bys>
  <columns>
    <column name="occurrence_id" display="Occurrence ID" datatype="integer" sql="o.id" />
    <column name="determination_id" display="Determination ID" datatype="integer" sql="d.id" />
    <column name="person_name" display="Determiner" datatype="text" sql="d.person_name" />
    <column name="taxon" display="Determined as taxon" datatype="text" sql="cttl.taxon" />
    <column name="common_name" display="Common name" datatype="text" sql="cttl.default_common_name" />
    <column name="accepted_name" display="Accepted name" datatype="text" sql="cttl.preferred_taxon" />
  </columns>
</report>